    <!-- @include 'header.kit' -->
    <section class="contact">
        <h2 class="section-title">Kontakt</h2>
            <div class="container">
                <form action="#" method="get" class="contact__form">

                    <div class="contact__input-box">
                        <input type="text" class="contact__input" id="name" name="name" required>
                        <label for="name" class="contact__label">Imię</label>
                    </div>

                    <div class="contact__input-box">
                        <input type="text" class="contact__input" id="email" name="email" required>
                        <label for="email" class="contact__label">Email</label>
                    </div>

                    <div class="contact__input-box">
                        <input type="text" class="contact__input" id="topic" name="topic" required>
                        <label for="topic" class="contact__label">Temat</label>
                    </div>

                    <div class="contact__input-box">
                        <textarea name="msg" id="msg" class="contact__input contact__input--textarea" required></textarea>
                        <label for="msg" class="contact__label">Wiadomość</label>
                    </div>

                    <button class="contact__button" type="submit">Wyślij ✉️</button>
                </form>
            </div>
    </section>
    <section class="google-map">
        <h2 class="section-title">Nasza lokalizacja</h2>
        <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d493.4874466836824!2d19.89268592023142!3d50.08823095621057!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x47165a2d7f0d587f%3A0x15f4e7c36adff6d2!2sJasnog%C3%B3rska%202%2C%2031-342%20Krak%C3%B3w!5e0!3m2!1spl!2spl!4v1623337263189!5m2!1spl!2spl" title="google_map" class="google-map__iframe" allowfullscreen="" loading="lazy"></iframe>
    </section>

    <!-- @include 'footer.kit' -->

    <script src="./dist/js/main.min.js" defer></script>
</body>
</html>